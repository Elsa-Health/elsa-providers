{
  "name": "providers",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint index.js app storybook test --fix --ext .js,.ts,.tsx",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write '**/*.js'",
    "format:json": "prettier --write '**/*.json'",
    "format:md": "prettier --write '**/*.md'",
    "format:ts": "prettier --write '**/*.ts{,x}'",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "patch": "patch-package",
    "postinstall": "node ./bin/postInstall",
    "prepare": "npm-run-all patch hack:*",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/progress-bar-android": "^1.0.4",
    "@react-native-firebase/app": "^11.1.2",
    "@react-native-firebase/auth": "^11.1.2",
    "@react-native-firebase/firestore": "^11.1.2",
    "@react-navigation/drawer": "^5.12.4",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@testing-library/react-native": "^7.2.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "apisauce": "^2.0.1",
    "axios": "^0.21.1",
    "bayesian-logistic-regressor": "^0.1.0",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "lodash.throttle": "4.1.1",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "mobx-state-tree": "^5.0.1",
    "moment": "^2.29.1",
    "ramda": "^0.27.0",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-animatable": "^1.3.3",
    "react-native-calendars": "^1.1254.0",
    "react-native-camera": "^3.43.0",
    "react-native-chart-kit": "^6.11.0",
    "react-native-code-push": "^7.0.0",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keychain": "6.2.0",
    "react-native-localize": "^2.0.2",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-orientation-locker": "^1.2.0",
    "react-native-paper": "^4.7.2",
    "react-native-permissions": "^3.0.1",
    "react-native-places-input": "^1.1.7",
    "react-native-prompt-android": "^1.1.0",
    "react-native-qrcode-scanner": "^1.5.3",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-safe-area-view": "1.1.1",
    "react-native-screens": "^2.18.1",
    "react-native-sectioned-multi-select": "renrizzolo/react-native-sectioned-multi-select#185/head",
    "react-native-splash-screen": "3.2.0",
    "react-native-sticky-parallax-header": "^0.4.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^8.1.0",
    "reactotron-mst": "^3.1.1",
    "reactotron-react-native": "^5.0.0",
    "validate.js": "0.13.1",
    "vasern": "^0.3.68",
    "zustand": "^3.3.3",
    "zustand-persist": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-optional-catch-binding": "^7.13.8",
    "@babel/runtime": "^7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/ramda": "0.27.39",
    "@types/react": "17.0.3",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-react-native": "^3.10.0",
    "eslint-plugin-standard": "^5.0.0",
    "ignite-bowser": "^5.4.1",
    "jest": "^26.6.3",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.65.2",
    "npm-run-all": "4.1.5",
    "patch-package": "6.4.7",
    "postinstall-prepare": "2.0.0",
    "prettier": "2.2.1",
    "react-devtools-core": "4.10.1",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "solidarity": "3.0.4",
    "typescript": "4.2.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./__mocks__/mockFirebase.ts",
      "./__mocks__/mockRNPermissions.ts",
      "./__mocks__/mockRNNavigation.ts"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/test/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-native|(@react-native|react-native)/|@react-navigation|@storybook|@react-native-community|vasern)"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "tabWidth": 4,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "comma-dangle": 0,
      "no-unused-vars": 0,
      "no-undef": 0,
      "quotes": 0,
      "indent": "off",
      "react/no-unescaped-entities": 0,
      "react/prop-types": "off",
      "react-native/no-raw-text": 0,
      "space-before-function-paren": 0,
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/indent": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-var-requires": 0
    }
  }
}
